{
  "manifest_version": 2,
  "name": "essay-outline",
  "version": "1.0.0",
  "description": "a chrome extension, generate essay-outline on all sites",
  "homepage_url": "https://github.com/yes1am/essay-outline",
  "author": "songjp1996@gmail.com",
  "icons": {
    "16": "./built/img/icon.png",
    "48": "./built/img/icon.png",
    "128": "./built/img/icon.png"
  },
  "browser_action": {
		"default_icon": "./built/img/icon.png",
		"default_title": "essay-outline",
		"default_popup": "./built/popup.html"
	},
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["./built/index.js"],
    "css": ["./built/css/index.css"],
    "run_at": "document_start"
  }],
  "options_ui":{
    "page": "./built/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage"
  ]
}