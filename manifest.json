{
  "manifest_version": 2,
  "name": "Github Markdown Outline Extension",
  "version": "1.0.0",
  "description": "Displays a clickable outline for markdown documents on Github",
  "homepage_url": "https://github.com/yes1am/github-markdown-outline-extension",
  "author": "songjp1996@gmail.com",
  "icons": {
    "16": "./built/img/icon.png",
    "48": "./built/img/icon.png",
    "128": "./built/img/icon.png"
  },
  "browser_action": 
	{
		"default_icon": "./built/img/icon.png",
		"default_title": "Github Markdown Outline Extension",
		"default_popup": "./built/popup.html"
	},
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["./built/index.js"],
    "css": ["./built/css/index.css"]
  }],
  "permissions": [
    "storage"
  ]
}